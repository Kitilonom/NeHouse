<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nehouse.nehouse">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Calendar"
            android:label="Calendar">
        </activity>

        <activity android:name=".ForgotPassword"
            android:label="Forgot Password">
        </activity>

        <activity android:name=".LogOn"
            android:label="Log On">
        </activity>

        <activity android:name=".PasswordChanging"
            android:label="Password Changing">
        </activity>

        <activity android:name=".Profile"
            android:label="Profile">
        </activity>

        <activity android:name=".WelcomePage"
            android:label="Welcome Page">
        </activity>

    </application>

</manifest>
